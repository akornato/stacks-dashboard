// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Transaction {
  user String
  tx_id   String @id
  block_height Int?
  tx_status String
  tx_type String
  token_transfer_amount String?

  @@index([user])
}
